		"matthiasmullie/minify": "1.3.59",
		"fortawesome/font-awesome": "4.7.0",
		"idangerous/Plugins": "dev-master#c832ac40b9b60133376c920d2eaa7195c2026e62",
		"fancyapps/fancybox": "2.1.6",
		"fengyuanchen/cropper": "4.0.0-beta",
