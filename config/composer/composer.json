			"type": "package",
			"package": {
				"name": "fengyuanchen/cropper",
				"version": "4.0.0-beta",
				"source": {
					"type": "git",
					"url": "https://github.com/fengyuanchen/cropper",
					"reference": "master"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "IanLunn/Hover",
