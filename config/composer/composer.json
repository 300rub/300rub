					"reference": "master"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "lodash/lodash",
				"version": "4.17.5",
				"source": {
					"type": "git",
					"url": "https://github.com/lodash/lodash.git",
					"reference": "master"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "vanderlee/colorpicker",
				"version": "1.2.16",
				"source": {
					"type": "git",
					"url": "https://github.com/vanderlee/colorpicker",
					"reference": "master"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "less/less",
				"version": "2.7.2",
				"source": {
					"type": "git",
					"url": "https://github.com/less/less.js",
					"reference": "master"
				}
			}
		},
		{
			"type": "package",
