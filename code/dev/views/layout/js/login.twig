<script>
    !function ($, ss) {
        window.jQuery(document).ready(function() {
            ss.init("app").setLanguage({{ language }});

            {% for key, value in errorMessages %}
                ss.init("commonComponentsError").set("{{ key }}", "{{ value }}");
            {% endfor %}

            ss.init("commonUserLogin");
        });
    }(window.jQuery, window.ss);
</script>
