					"type": "git",
					"url": "https://github.com/troolee/gridstack.js.git",
					"reference": "master"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "lodash/lodash",
				"version": "4.17.5",
				"source": {
					"type": "git",
					"url": "https://github.com/lodash/lodash.git",
					"reference": "master"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "vanderlee/colorpicker",
				"version": "1.2.16",
				"source": {
					"type": "git",
					"url": "https://github.com/vanderlee/colorpicker",
