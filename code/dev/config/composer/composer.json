				"source": {
					"type": "git",
					"url": "https://github.com/IanLunn/Hover",
					"reference": "master"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "blueimp/JavaScript-MD5",
				"version": "2.10.0",
				"source": {
					"type": "git",
					"url": "https://github.com/blueimp/JavaScript-MD5",
					"reference": "master"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "fancyapps/fancybox",
				"version": "3.2.5",
				"source": {
					"type": "git",
					"url": "https://github.com/fancyapps/fancybox",
					"reference": "master"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "jssor/slider",
				"version": "27.1.0",
				"source": {
					"type": "git",
					"url": "https://github.com/jssor/slider",
