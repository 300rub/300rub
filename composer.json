{
  "require": {
    "troolee/gridstack": "dev-master",
    "lodash/lodash": "dev-master",
    "tinymce/tinymce": "dev-master",
    "vanderlee/colorpicker": "dev-master",
    "less/less": "dev-master"
  },
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "troolee/gridstack",
        "version": "dev-master",
        "source": {
          "type": "git",
          "url": "https://github.com/troolee/gridstack.js.git",
          "reference": "master"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "lodash/lodash",
        "version": "dev-master",
        "source": {
          "type": "git",
          "url": "https://github.com/lodash/lodash.git",
          "reference": "master"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "vanderlee/colorpicker",
        "version": "dev-master",
        "source": {
          "type": "git",
          "url": "https://github.com/vanderlee/colorpicker",
          "reference": "master"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "less/less",
        "version": "dev-master",
        "source": {
          "type": "git",
          "url": "https://github.com/less/less.js",
          "reference": "master"
        }
      }
    }
  ],
  "config": {
    "vendor-dir": "vendors",
    "discard-changes": true
  }
}